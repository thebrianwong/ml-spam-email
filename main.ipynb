   "source": [
    "# Encode text into integer tokens for NLP"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "def tokenize(data_object: EmailDataSet):\n",
    "    # Create NLP vocabulary based on training emails\n",
    "    encoder = tf.keras.layers.experimental.preprocessing.TextVectorization(max_tokens=20000)\n",
    "    vocab_data = data_object.training_emails\n",
    "    encoder.adapt(vocab_data)\n",
    "\n",
    "    return encoder\n",
    "\n",
    "class Encoder():\n",
    "    encoder = None\n",
    "\n",
    "encoder_obj = Encoder()\n",
    "\n",
    "encoder_obj.encoder = tokenize(email_data_set)"
   ]
  },
